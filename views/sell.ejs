             </div>
        <div id="user-form">
            <div id="sell-products">
                <div class="sell-center">
                    <% for(product of products){ %>
                        <div id="product" style="cursor:pointer" onclick="document.location='/admin/manage_products'">
                            <%  %>
                            <%= product.productName %>
                            <br>
                            ₦<%= product.sellingPrice %>
                            <br>
                            <%= product.productQuantity %>
                        </div>
                        <% } %>
                        <div class="sell-nav">
                            <nav aria-label="...">
                                <ul class="pagination">
                                  <li class="page-item disabled">
                                    <a class="page-link" href="#" tabindex="-1">Previous</a>
                                  </li>
                                  <li class="page-item"><a class="page-link" href="#">1</a></li>
                                  <li class="page-item active">
                                    <a class="page-link" href="#">2 <span class="sr-only">(current)</span></a>
                                  </li>
                                  <li class="page-item"><a class="page-link" href="#">3</a></li>
                                  <li class="page-item">
                                    <a class="page-link" href="#">Next</a>
                                  </li>
                                </ul>
                              </nav>
                        </div>
                </div>
            </div>
            <div id="add-user">
                <div id="items-to-sell">
                   
                </div>
                <div class="sell-form-footer">
                    <div class="sell-footer-btn">
                        <button type="reset" id="clear" class="btn btn-sm btn-outline-success"><i class="fas fa-times"></i></button>
                        <button type="reset" id="remove" class="btn btn-sm btn-outline-success"><i class="fas fa-minus"></i></button>
                        <button type="reset" id="remove" class="btn btn-sm btn-outline-success"><i class="fas fa-minus"></i></button>
                    </div>
                    
                    <form class="form" action="/admin/manage_users/add_user" method="POST">
                        <p>TOTAL: <span>₦</span></p>
                    </form>
                </div>
            </div>
           
        </div>
    </div>
<div>
    <div>
       <footer id="footer">
          <% include partials/footer %>
       </footer>
    </div>
</div>
</body>
</html>